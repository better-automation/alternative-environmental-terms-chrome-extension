{
    "manifest_version": 2,
    "name": "Alternative Environmental Terms",
    "description": "Changes existing environmental terms to better alternative terms.",
    "version": "1.1.0",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
